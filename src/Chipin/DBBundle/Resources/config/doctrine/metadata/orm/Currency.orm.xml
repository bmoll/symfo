<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Currency" table="currency">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="displayname" type="string" column="displayName" length="255" nullable="false"/>
    <field name="decimalmark" type="string" column="decimalMark" length="1" nullable="false"/>
    <field name="unitname" type="string" column="unitName" length="255" nullable="false"/>
    <field name="centname" type="string" column="centName" length="255" nullable="false"/>
    <field name="unitsymbol" type="string" column="unitSymbol" length="255" nullable="false"/>
    <field name="centsymbol" type="string" column="centSymbol" length="255" nullable="false"/>
    <field name="symbol" type="string" column="symbol" length="255" nullable="false"/>
    <field name="symbolposition" type="integer" column="symbolPosition" nullable="false"/>
    <field name="datecreated" type="datetime" column="dateCreated" nullable="false"/>
    <field name="dateedited" type="datetime" column="dateEdited" nullable="false"/>
    <field name="datedeleted" type="datetime" column="dateDeleted" nullable="true"/>
  </entity>
</doctrine-mapping>
